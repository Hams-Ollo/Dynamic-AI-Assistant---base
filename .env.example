# System Configuration
APP_ENV=development
DEBUG=false
LOG_LEVEL=INFO

# Security
MAX_MEMORY_MB=512
MAX_CPU_PERCENT=50
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_SECONDS=60
AGENT_API_KEY=your_api_key_here
AGENT_JWT_SECRET_KEY=your_jwt_secret_key_here

# LLM Configuration
LLM_API_KEY=your_groq_api_key_here
LLM_MODEL=llama3-groq-70b-8192-tool-use-preview
LLM_TEMPERATURE=0.7

# Memory Configuration
MEMORY_TYPE=vector
MEMORY_BACKEND=chroma
MEMORY_PATH=./data/memory

# OpenAI Configuration (for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# API Configuration
API_HOST=localhost
API_PORT=8000

# Logging
AGENT_LOG_FILE=logs/app.log
AGENT_LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Agent Specific Settings
AGENT_DEFAULT_MODEL=gpt-3.5-turbo
AGENT_DEFAULT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=150
