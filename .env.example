# System Configuration
AGENT_LOG_LEVEL=INFO
AGENT_ENVIRONMENT=development
AGENT_DEBUG=false

# Security
AGENT_API_KEY=your_api_key_here
AGENT_JWT_SECRET_KEY=your_jwt_secret_key_here

# LLM Configuration
AGENT_LLM_PROVIDER=openai
AGENT_OPENAI_API_KEY=your_openai_api_key_here

# Memory Configuration
AGENT_MEMORY_TYPE=vector
AGENT_MEMORY_BACKEND=chroma

# Database Configuration
AGENT_DB_URL=sqlite:///data/memory.db

# API Configuration
AGENT_API_HOST=localhost
AGENT_API_PORT=8000

# Rate Limiting
AGENT_RATE_LIMIT_REQUESTS=100
AGENT_RATE_LIMIT_SECONDS=60

# Logging
AGENT_LOG_FILE=logs/app.log
AGENT_LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Agent Specific Settings
AGENT_DEFAULT_MODEL=gpt-3.5-turbo
AGENT_DEFAULT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=150

# Security Limits
AGENT_MAX_MEMORY_MB=512
AGENT_MAX_CPU_PERCENT=50
